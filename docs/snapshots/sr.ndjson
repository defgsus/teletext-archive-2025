[["__"," 118      SAARTEXT    Di.12.08. 10:20:02"]]
[["__","                            NACHRICHTEN "]]
[["__","                                   Perl "]]
[["__","                                        "]]
[["__"," \"Top\"-Weinjahrgang im Saarland erwartet"]]
[["__","                                        "]]
[["__"," Der saarländische Winzerpräsident Hoff-"]]
[["__"," mann erwartet für dieses Jahr einen    "]]
[["__"," sehr guten Wein-Jahrgang. Die Wetterbe-"]]
[["__"," dingungen und Reifeentwicklung seien   "]]
[["__"," ideal.                                 "]]
[["__","                                        "]]
[["__"," Das lasse auf einen exzellenten Jahr-  "]]
[["__"," gang hoffen. Das Wetter in den nächsten"]]
[["__"," Wochen sei entscheidend. Im Moment sehe"]]
[["__"," alles gut aus, aber das Blatt könne    "]]
[["__"," sich schnell wenden.                   "]]
[["__","                                        "]]
[["__"," Rund 30 Winzer im Saarland bewirtschaf-"]]
[["__"," ten demnach eine Fläche von rund 130   "]]
[["__"," Hektar. Für die nächsten Wochen wünscht"]]
[["__"," sich der Winzerpräsident sonnige Tage  "]]
[["__"," und kühle Nächte.                      "]]
