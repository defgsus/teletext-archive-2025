[["__"," 163      SAARTEXT    Di.12.08. 10:20:02"]]
[["__","                         SAARLANDWETTER "]]
[["__","                    Aussichten/Prognose "]]
[["__","                                    1/2 "]]
[["__"," Zunehmend schwül und Hitzegewitter     "]]
[["__","                                        "]]
[["__"," Am Donnerstag sehr hohe Wärmebelastung "]]
[["__"," und zunehmende Schwüle. Dazu Wechsel   "]]
[["__"," aus Sonne und Wolken. Meist trocken,   "]]
[["__"," vereinzelt Hitzegewitter. In der Frühe "]]
[["__"," 16°C bis 23°C, am Tag 33°C bis 37°C.   "]]
[["__","                                        "]]
[["__"," Am Freitag nach teils tropisch warmer  "]]
[["__"," Nacht Sonne, Quellwolken und einzelne  "]]
[["__"," Hitzegewitter. Tiefstwerte bei 17°C bis"]]
[["__"," 23°C, Höchstwerte bei 31°C bis 35°C. Am"]]
[["__"," Samstag sonnig und wenige Wolken.      "]]
[["__","                                        "]]
[["__"," Es bleibt trocken. Am Morgen 15°C bis  "]]
[["__"," 20°C, tagsüber 28°C bis 32°C. Am Sonn- "]]
[["__"," tag Wechsel aus Sonne und lockeren Wol-"]]
[["__"," ken und trocken. Frühwerte von 13°C bis"]]
[["__"," 17°C, Tageswerte von 25°C bis 30°C.    "]]
[["__","                                     "],["__",[163,2],">>"],["__"," "]]
