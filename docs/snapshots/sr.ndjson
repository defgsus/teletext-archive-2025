[["__","                            Saarbrücken "]]
[["__","                                        "]]
[["__"," Übersetzerpreis für Elisabeth Edl      "]]
[["__","                                        "]]
[["__"," Der Eugen-Helmle-Übersetzerpreis geht  "]]
[["__"," in diesem Jahr an die Übersetzerin Eli-"]]
[["__"," sabeth Edl. Nach Ansicht der Jury stel-"]]
[["__"," le sie sich immer wieder mit Vergnügen "]]
[["__"," den Herausforderungen der Übersetzung. "]]
[["__","                                        "]]
[["__"," Dabei überzeugten ihre Übersetzungen   "]]
[["__"," aus dem Französischen durch ihren ei-  "]]
[["__"," nerseits präzisen und andererseits     "]]
[["__"," freien und kreativen Umgang mit der    "]]
[["__"," Sprache.                               "]]
[["__","                                        "]]
[["__"," Der mit 10.000 Euro dotierte Preis, den"]]
[["__"," der SR mit der Stiftung ME Saar und der"]]
[["__"," Stadt Sulzbach vergibt, erinnert an den"]]
[["__"," Übersetzer Eugen Helmle. Die Preisver- "]]
[["__"," leihung ist am 9.September in Sulzbach."]]
[["__","                                     "],["__",118,">>"],["__"," "]]
